{
  "name": "mohsenfathipour/captcha-generator",
  "description": "A customizable CAPTCHA generator for Laravel applications.",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0",
    "illuminate/http": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "MohsenFathipour\\CaptchaGenerator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MohsenFathipour\\CaptchaGenerator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Mohsen Fathipour",
      "email": "mohsenfathipour.com@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mohsenfathipour/captcha-generator"
    }
  ]
}
