{
  "name": "mohsenfathipour/captcha-generator",
  "description": "A Laravel package for generating CAPTCHA images.",
  "keywords": ["captcha", "captcha-generator", "laravel", "image"],
  "license": "MIT",
  "authors": [
    {
      "name": "MohsenFathipour",
      "email": "mohsenfathipour.com@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MohsenFathipour\\CaptchaGenerator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
